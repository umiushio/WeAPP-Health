/* pages/manage/manage.wxss */
view .nav-bar{
  background-color: skyblue;
}
.nav-bar-title{
  padding-bottom: 50rpx;
  font-size: xx-large;
  font-weight: bold;
}
.nav-bar-user{
  color: dodgerblue;
  display: flex;
  flex-direction: row;
  font-size: larger;
  font-weight: bold;
}
.cell-value-display{
  display: flex;
  flex-direction: column;
  text-align: center;
  font-weight: bold;
  color: black;
  font-size: 40rpx;
}
.cell-value-label{
  font-weight: normal;
  font-size: 30rpx;
}
.cell-title-display{
  display: flex;
  flex-direction: row;
}
.cell-title-text{
  padding-right: 20rpx;
  font-size: 40rpx;
  font-weight: bold;
}
.view-content{
  background-color: whitesmoke;
  padding: 15rpx;
}
.button-menu{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  padding: 15rpx;
}